{
	"name": "stanislav-janu/database-updater",
	"description": "Simple manager of database changes.",
	"homepage": "https://janu.software",
	"license": ["MIT"],
	"authors": [
		{
			"name": "Stanislav JanÅ¯",
			"homepage": "https://janu.software"
		}
	],
	"autoload": {
		"classmap": ["src/"]
	},
	"require": {
		"php": ">=8.0",
		"nette/database": "^3.1 || ^4",
		"nette/utils": "^3.2 || ^4",
		"latte/latte": "^2.9 || ^3",
		"tracy/tracy": "^2.7 || ^4"
	},
	"require-dev": {
		"rector/rector": "^0.12",
		"stanislav-janu/phpstan": "^1.0"
	},
	"minimum-stability": "stable",
	"extra": {
		"branch-alias": {
			"dev-master": "3.0-dev"
		}
	},
	"scripts": {
		"sa": "vendor/bin/phpstan.phar analyse --ansi",
		"sacc": "vendor/bin/phpstan.phar clear-result-cache",
		"r": "vendor/bin/rector process src --dry-run --ansi",
		"r-f": "vendor/bin/rector process src --ansi"
	}
}
